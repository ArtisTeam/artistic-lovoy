<div class="container">
  <h2>Organization Dashboard</h2>
  <a href="/event/new">Create event</a>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Updated</th>
        <th>Created</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i=0; i<events.length ; ++i) {%>
        <% var event = events[i] %>
        <tr>
          <td><%= event.get('eventName')%></td>
          <td><%= event.get('eventDescription')%></td>
          <td><%= event.updatedAt%></td>
          <td><%= event.createdAt%></td>
          <td>
            <form action="/event/<%= event.id %>/edit" method="get">
              <input type="submit" value="edit">
            </form>
          </td>
          <td>
            <form action="/event/<%= event.id %>/delete" method="post">
              <input type="submit" value="delete"/>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>